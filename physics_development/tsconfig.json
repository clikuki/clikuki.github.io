{
	"include": [
		"../src/physics/*.ts"
	],
	"compilerOptions": {
		"target": "es2024",
		"module": "es2022",
		"rootDir": "../src/physics",
		"outDir": "./js",
		"sourceMap": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": false,
		"alwaysStrict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"skipLibCheck": true
	}
}
